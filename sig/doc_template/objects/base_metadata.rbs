module DocTemplate
  module Objects
    class BaseMetadata
      include Virtus::InstanceMethods::Constructor
      extend Virtus::InstanceMethods::_Constructor

      def self.build_from: (untyped data) -> untyped

      #
      # Splits the text by separator removing empty parts
      #
      # @param text [String] text to be split
      # @param separator [String]
      # @return [Array] array of parts
      #
      def self.split_field: (untyped text, ?untyped separator) -> untyped
    end
  end
end
